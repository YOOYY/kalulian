doctype html
html(lang=lang)
    block config
    head
        meta(charset="utf-8")
        meta(name="viewport",content="width=device-width, initial-scale=1.0")
        meta(http-equiv="X-UA-Compatible",content="ie=edge,chrome=1")
        title #{cnTitle}#{title}
        meta(name="keywords",content=cnTitle + keywords)
        meta(name="description",content=cnTitle + description)
        meta(name="format-detection" content="telephone=no")
        link(rel="shortcut icon" href=imgUrl+"favicon.ico")
        link(rel='stylesheet',href=cssUrl + 'index.css')
        link(rel='stylesheet',href=cssUrl + 'animate.min.css')
        link(rel='stylesheet',href='https://unpkg.com/swiper/swiper-bundle.min.css')
        script(src=jsUrl+"flexible.js")
        block css
        <!--[if lte IE 8]>
        <script src="https://cdn.bootcdn.net/ajax/libs/rem/1.3.4/js/rem.js" type="text/javascript"></script>
        
        <![endif]-->
    block body